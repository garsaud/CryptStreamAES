{
    "name": "garsaud/cryptstreamaes",
    "type": "library",
    "license": "MIT",
    "description": "Encrypt and decrypt php resources (streams) on-the-fly using AES 128, 192, 256",
    "require": {
        "php": ">=8.1",
        "ext-mcrypt": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Garsaud\\CryptStreamAES\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garsaud\\CryptStreamAES\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
